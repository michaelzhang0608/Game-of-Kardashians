    "def get_speakers(speakers, speaker_dic, gender):\n",
    "    m_total = len(speaker_dic[speakers[0]])\n",
    "    f_total = len(speaker_dic[speakers[1]])\n",
    "    arr = ['TYRION', 'CERSEI']\n",
    "    speaker_index = 2\n",
    "    while abs(m_total - f_total) > 150:\n",
    "        speaker = speakers[speaker_index]\n",
    "        if m_total > f_total and gender[speaker] == 'F':\n",
    "            f_total+=len(speaker_dic[speaker])\n",
    "            arr.append(speaker) \n",
    "        elif m_total < f_total and gender[speaker]== 'M':\n",
    "            m_total+=len(speaker_dic[speaker])\n",
    "            arr.append(speaker)\n",
    "        speaker_index+=1\n",
    "    return arr\n",
    "            \n",
    "speakers = get_speakers(speakers, speaker_dic, genders)\n",
    "print(\"Here are the speakers we will analyze: \" + str(speakers))"
   ]
